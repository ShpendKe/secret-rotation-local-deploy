<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <RootNamespace>SecretRotationExtension</RootNamespace>
        <AssemblyName>secret-rotation-extension</AssemblyName>
        <IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
        <PublishSingleFile>true</PublishSingleFile>
        <SelfContained>true</SelfContained>
        <InvariantGlobalization>true</InvariantGlobalization>
        <TargetFramework>net9.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>
    <ItemGroup>
        
    <PackageReference Include="Azure.Bicep.Local.Extension" Version="0.39.26" />
    <PackageReference Include="Azure.Identity" Version="1.17.1" />
    <PackageReference Include="Bicep.LocalDeploy" Version="1.0.1" />
    <PackageReference Include="Microsoft.Graph" Version="5.102.0" />
    <PackageReference Include="Nerdbank.GitVersioning" Version="3.9.50" />
    </ItemGroup>
</Project>
